{
  "expo": {
    "name": "QuestOS",
    "slug": "questos",
    "version": "0.1.0",
    "orientation": "portrait",
    "backgroundColor": "#130F12",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#130F12"
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBar": {
      "backgroundColor": "#130F12",
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "backgroundColor": "#130F12",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "backgroundColor": "#130F12"
      }
    }
  }
}
